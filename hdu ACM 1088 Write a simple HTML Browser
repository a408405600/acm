#include<stdio.h>
#include<string.h>
int main()
{
	freopen("debug\\ZOJ_3775.txt","r",stdin);
	char s[82];
	int flag=1;
	while(~scanf("%s",&c))
	{
		if(strcmp(c,"<br>")){
			flag=0;
			printf("\n");
		}else if(strcmp(c,"<hr>")){
			if(flag){
				pritnf("--------------------------------------------------------------------------------\n");
			}else{
				pritnf("\n--------------------------------------------------------------------------------\n");
			}
			
		}else{
			printf("%s",s);
		}
	}
}